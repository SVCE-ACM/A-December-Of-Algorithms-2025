# Employee Skill Balancing

N = int(input("Enter number of employees: "))
skills = list(map(int, input("Enter skill values: ").split()))

total = sum(skills)
half = total // 2

# DP array to track achievable sums up to half of total
dp = [[False] * (half + 1) for _ in range(N + 1)]
dp[0][0] = True

for i in range(1, N + 1):
    for j in range(half, skills[i-1] - 1, -1):
        dp[i][j] |= dp[i-1][j - skills[i-1]]

best = 0
for j in range(half, -1, -1):
    if dp[N][j]:
        best = j
        break

min_difference = total - 2 * best
print(min_difference)

