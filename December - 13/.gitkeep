# Find mountain peaks in a hiking trail

N = int(input().strip())
heights = list(map(int, input().split()))

peaks = []

# First and last cannot be peaks
for i in range(1, N - 1):
    if heights[i] > heights[i - 1] and heights[i] > heights[i + 1]:
        peaks.append(i)

if len(peaks) == 0:
    print(-1)
else:
    print(*peaks)

