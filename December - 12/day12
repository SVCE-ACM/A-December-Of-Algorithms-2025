n = int(input())
arr = list(map(int, input().split()))

i = 0
while i < n:
    correct = arr[i] - 1
    if arr[i] != arr[correct]:
        arr[i], arr[correct] = arr[correct], arr[i]
    else:
        i += 1

missing = duplicate = -1
for i in range(n):
    if arr[i] != i + 1:
        missing = i + 1
        duplicate = arr[i]
        break

print(f"Missing Number: {missing}")
print(f"Duplicate Number: {duplicate}")
